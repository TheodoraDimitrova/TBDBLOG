

<div class="container">
<div id="profile-page" class="section">
            <!-- profile-page-header -->
            <div id="profile-page-header" class="card">
               <div>
                     <figure class="card-profile-image">
                    <img src="data:image/png;base64," alt="profile image" class="circle z-depth-2 responsive-img activator">
                   </figure>
               </div>
                <div class="card-content">
                  <div class="row">                    
                    <div class="col s2 offset-s2">                        
                        <h4 class="card-title grey-text text-darken-4">Name</h4>
                        <p class="medium-small grey-text">{{user.name}} {{user.last}}</p>                        
                    </div>
                    <div class="col s2 center-align">
                        <h4 class="card-title grey-text text-darken-4">Email</h4>
                        <p class="medium-small grey-text">{{user.email}}</p>                        
                    </div>
                    <div class="col s2 center-align">
                        <h4 class="card-title grey-text text-darken-4">Questions</h4>
                        <p class="medium-small grey-text"> {{questions.length}}</p>                        
                    </div>                    
                  </div>
                </div>
            </div>
            

  
            




























         